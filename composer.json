{
	"name":"maximeculea/composer-stable-versions",
	"type":"composer-plugin",
	"minimum-stability":"stable",
	"license":"GPL-3.0+",
	"description":"Make all your composer's dependencies stable.",
	"keywords":[
		"composer",
		"composer-script",
		"package",
		"stable",
		"versions"
	],
	"authors":[
		{
			"name":"Maxime Culea",
			"email":"contact@maximeculea.fr",
			"homepage":"https://maximeculea.fr"
		}
	],
	"require":{
		"php":">=5.6",
		"composer-plugin-api":"^2.0"
	},
	"require-dev":{
		"composer/composer":"^2.0"
	},
	"autoload":{
		"psr-4":{
			"MaximeCulea\\ComposerScripts\\ComposerStableVersions\\":"src"
		}
	},
	"extra":{
		"class":"MaximeCulea\\ComposerScripts\\ComposerStableVersions\\StableVersions"
	}
}
